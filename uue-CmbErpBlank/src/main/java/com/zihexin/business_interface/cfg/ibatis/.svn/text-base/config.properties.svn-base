#银行――socket配置地址
abc.bank.connect.host=10.5.13.177
#端口号 交易端口： 3837 ，文件端口：3851
abc.bank.connect.port=1080
#线程池不可太小
abc.bank.connect.poolsize=3
abc.bank.connect.processorPoolSize=0
#连接超时时间秒
abc.bank.connect.connectSoTimeOut=3000
#请求发呆时间
abc.bank.connect.idleTime=10
#接受缓冲区大小
abc.bank.connect.readBufferSize=204800
#写超时,单位是秒
abc.bank.connect.writeTimeout=10



